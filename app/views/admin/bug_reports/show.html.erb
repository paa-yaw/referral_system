<div class="col-lg-12 col-md-12">
	<div class="card">
		<div class="card-header" data-background-color="red">
			<h4 class="title"><i class="material-icons">bug_report</i>#<%= @bug_report.id %>. 
				<%= @bug_report.error_message %></h4></h4>
			</div>
			<div class="card-content table-responsive">
				<table class="table table-hover">
					<thead class="text-warning">
						<th>Stacktrace</th>
					</thead>
					<tbody>
						<% @bug_report.stacktrace.each do |trace| %>
						<tr>	                                        	
							<td><%= trace %></td>
						</tr>                                
						<% end %>  
					</tbody>
				</table>
			</div>
		</div>
	</div>